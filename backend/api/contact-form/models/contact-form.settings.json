{
  "kind": "collectionType",
  "collectionName": "contact_forms",
  "info": {
    "name": "contact_form"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "email": {
      "type": "email",
      "required": true
    },
    "subject": {
      "type": "enumeration",
      "enum": [
        "school",
        "website",
        "education",
        "maths"
      ]
    },
    "name": {
      "type": "string",
      "required": true
    },
    "title": {
      "type": "enumeration",
      "enum": [
        "Mr",
        "Mrs",
        "Miss",
        "Ms",
        "Dr",
        "Sir"
      ]
    },
    "message": {
      "type": "text"
    }
  }
}
